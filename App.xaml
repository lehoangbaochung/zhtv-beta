<Application x:Class="zhtv.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="\Views\MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Dictionaries/ChatResource.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/Dictionaries/MusicResource.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/Dictionaries/MainResource.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
